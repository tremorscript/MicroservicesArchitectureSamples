// Place your key bindings in this file to override the defaultsauto[]
[
	{
		"key": "ctrl+alt+n",
		"command": "welcome.showNewFileEntries"
	},
	{
		"key": "ctrl+alt+win+n",
		"command": "-welcome.showNewFileEntries"
	},
	{
		"key": "ctrl+shift+alt+g",
		"command": "pr:github.toggleVisibility"
	},
	{
		"key": "ctrl+\\ ctrl+e",
		"command": "workbench.action.problems.focus"
	},
	{
		"key": "shift+escape",
		"command": "workbench.action.closePanel"
	},
	{
		"key": "shift+f2",
		"command": "explorer.newFile"
	},
	{
		"key": "ctrl+shift+alt+f",
		"command": "workbench.view.extension.ionide-fsharp"
	},
	{
		"key": "shift+alt+t",
		"command": "dotnet-test-explorer.openPanel"
	},
	{
		"key": "ctrl+f f",
		"command": "prettier.forceFormatDocument"
	},
	{
		"key": "f",
		"command": "explorer.newFolder",
		"when": "filesExplorerFocus && !inputFocus"
	},
	{
		"key": "d",
		"command": "deleteFile",
		"when": "filesExplorerFocus && !inputFocus"
	},
	{
		"key": "a",
		"command": "explorer.newFile",
		"when": "filesExplorerFocus && !inputFocus"
	},
	{
		"key": "y",
		"command": "filesExplorer.copy",
		"when": "filesExplorerFocus && !inputFocus"
	},
	{
		"key": "p",
		"command": "filesExplorer.paste",
		"when": "filesExplorerFocus && !inputFocus"
	},
	{
		"key": "x",
		"command": "filesExplorer.cut",
		"when": "filesExplorerFocus && !inputFocus"
	},
	{
		"key": "r",
		"command": "renameFile",
		"when": "filesExplorerFocus && !inputFocus"
	},
	{
		"key": "ctrl+h",
		"command": "workbench.action.focusActiveEditorGroup",
		"when": "workbench.explorer.openEditorsView.active || filesExplorerFocus && vim.active && vim.mode!='Insert'"
	},
	{
		"key": "ctrl+k",
		"command": "workbench.action.navigateUp",
		"when": "vim.active && vim.mode!='Insert'"
	},
	{
		"key": "ctrl+k",
		"command": "workbench.files.action.focusOpenEditorsView",
		"when": "filesExplorerFocus && vim.active && vim.mode!='Insert'"
	},
	{
		"key": "ctrl+l",
		"command": "workbench.action.navigateRight",
		"when": "vim.active && vim.mode!='Insert'"
	},
	{
		"key": "ctrl+j",
		"command": "workbench.action.navigateDown",
		"when": "vim.active && vim.mode!='Insert'"
	},
	{
		"key": "ctrl+h",
		"command": "workbench.action.navigateLeft",
		"when": "vim.active && vim.mode!='Insert'"
	},
	{
		"key": "ctrl+s h",
		"command": "editor.action.showHover",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+k ctrl+i",
		"command": "-editor.action.showHover",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+k ctrl+i",
		"command": "-editor.action.showHover",
		"when": "editorTextFocus"
	},
	{
		"key": "space o",
		"command": "filesExplorer.openFilePreserveFocus",
		"when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsFolder && !inputFocus"
	},
	{
		"key": "space",
		"command": "-filesExplorer.openFilePreserveFocus",
		"when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsFolder && !inputFocus"
	},
	{
		"key": "] d",
		"command": "editor.action.marker.next",
		"when": "editorFocus && vim.mode == 'Normal'"
	},
	{
		"key": "[ d",
		"command": "editor.action.marker.prev",
		"when": "editorFocus && vim.mode == 'Normal'"
	},
	{
		"key": "alt+f8",
		"command": "-editor.action.marker.next",
		"when": "editorFocus"
	},
	{
		"key": "ctrl+alt+s",
		"command": "workbench.files.action.focusFilesExplorer"
	},
	{
		"key": "ctrl+j",
		"command": "workbench.files.action.focusFilesExplorer",
		"when": "vim.active && workbench.explorer.openEditorsView.active && !editorFocus && vim.mode != 'Insert'"
	},
	{
		"key": "ctrl+s e",
		"command": "solutionExplorer.focus"
	},
	{
		"key": "ctrl+shift+alt+b",
		"command": "csdevkit.buildSolution",
		"when": "focusedView == 'solutionExplorer"
	}
]
